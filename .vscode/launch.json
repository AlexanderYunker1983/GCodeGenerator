{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (GCodeGenerator)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GCodeGenerator/bin/Debug/net9.0/GCodeGenerator.dll",
            "args": [],
            "cwd": "${workspaceFolder}/GCodeGenerator",
            "stopAtEntry": false,
            "env": {
                "DOTNET_ENVIRONMENT": "Development"
            },
            "osx": {
                "console": "integratedTerminal"
            },
            "windows": {
                "console": "internalConsole"
            },
            "linux": {
                "console": "integratedTerminal"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}

